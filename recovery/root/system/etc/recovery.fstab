# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

#<src>                                                  <mnt_point>			  <type>  <mnt_flags and options>                                                                         <fs_mgr_flags>
system                                                  /system                ext4    ro,barrier=1,discard                                                                            wait,slotselect,logical
product                                                 /product               ext4    ro,barrier=1,discard                                                                            wait,slotselect,logical
vendor                                                  /vendor                ext4    ro,barrier=1,discard                                                                            wait,slotselect,logical
system_ext                                              /system_ext            ext4    ro,barrier=1,discard                                                                            wait,slotselect,logical
vendor_dlkm                                             /vendor_dlkm           ext4    ro,barrier=1,discard                                                                            wait,slotselect,logical
system_dlkm                                             /system_dlkm           ext4    ro,barrier=1,discard                                                                            wait,slotselect,logical

system                                                  /system                f2fs    ro                                                                                              wait,slotselect,logical
system_dlkm                                             /system_dlkm           f2fs    ro                                                                                              wait,slotselect,logical

system                                                  /system                erofs   ro                                                                                              wait,slotselect,logical
product                                                 /product               erofs   ro                                                                                              wait,slotselect,logical
vendor                                                  /vendor                erofs   ro                                                                                              wait,slotselect,logical
odm                                                     /odm                   erofs   ro                                                                                              wait,slotselect,logical
system_ext                                              /system_ext            erofs   ro                                                                                              wait,slotselect,logical
system_dlkm                                             /system_dlkm           erofs   ro                                                                                              wait,slotselect,logical

# Data partition
/dev/block/by-name/metadata                             /metadata              f2fs    noatime,nosuid,nodev,discard,sync,fsync_mode=strict,data_flush	wait,formattable,check
/dev/block/by-name/userdata                             /data                  f2fs    noatime,nosuid,nodev,discard,usrquota,grpquota,fsync_mode=nobarrier,reserve_root=32768,resgid=5678,whint_mode=fs-based,inlinecrypt  latemount,wait,check,quota,reservedsize=128M,checkpoint=fs,fscompress,fileencryption=aes-256-xts:aes-256-cts:v2+inlinecrypt_optimized+wrappedkey_v0,metadata_encryption=aes-256-xts:wrappedkey_v0,keydirectory=/metadata/vold/metadata_encryption

# Misc
/dev/block/by-name/misc                                 /misc                  emmc    defaults		defaults,first_stage_mount
